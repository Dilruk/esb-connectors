<proxy xmlns="http://ws.apache.org/ns/synapse"
       name="podio_uploadFile"
       transports="https,http"
       statistics="disable"
       trace="disable"
       startOnLoad="true">
   <target>
      <inSequence>
         <podio.uploadFile>
            <apiUrl>{$url:apiUrl}</apiUrl>
         </podio.uploadFile>
         <respond/>
      </inSequence>
      <outSequence>
         <log/>
         <send/>
      </outSequence>
   </target>
   <description/>
</proxy>                                              